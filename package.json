{
  "name": "nodejs-ts-template",
  "version": "0.0.1",
  "description": "A tool used to generate fake Google Form answers.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "Samy Ouyahia",
  "email": "sr.ouyahia@gmail.com",
  "url": "",
  "keywords": [
    "google",
    "form",
    "forms",
    "answer",
    "answers",
    "generator",
    "random",
    "faker"
  ],
  "license": "ISC",
  "scripts": {
    "prebuild": "npm run clean",
    "build": "tsc",
    "pretest": "npm run build",
    "test": "jest",
    "pretest:coverage": "npm run build",
    "test:coverage": "jest --coverage",
    "clean": "rimraf dist && rimraf output && mkdirp dist && mkdirp output",
    "lint": "npm run eslint && npm run prettier",
    "lint:fix": "npm run eslint:fix && npm run prettier:fix",
    "eslint": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src ./spec",
    "eslint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src ./spec",
    "prettier:base": "prettier --parser typescript --single-quote",
    "prettier": "npm run prettier:base -- --list-different \"src/**/*.{ts,tsx}\" \"spec/**/*.{ts,tsx}\"",
    "prettier:fix": "npm run prettier:base -- --write \"src/**/*.{ts,tsx}\" \"spec/**/*.{ts,tsx}\"",
    "prestart": "npm run build",
    "start": "node dist/src/index.js | ./node_modules/.bin/bunyan"
  },
  "devDependencies": {
    "@types/bunyan": "^1.8.6",
    "@types/faker": "^5.5.3",
    "@types/jest": "^26.0.23",
    "@types/node": "^14.14.43",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^3.10.1",
    "eslint": "^7.25.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-airbnb-typescript": "^9.0.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-formatter-pretty": "^4.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^23.20.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "jest": "^26.6.3",
    "mkdirp": "^1.0.4",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.5.5"
  },
  "dependencies": {
    "ajv": "^8.2.0",
    "bunyan": "^1.8.15",
    "faker": "^5.5.3",
    "typescript": "^4.2.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/souyahia/gform-data-generator.git"
  },
  "private": false
}
